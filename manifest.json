{
    "manifest_version": 3,
    "name": "Laz Redirects",
    "version": "1.3",
    "description": "Redirects links to Kemono Party",
    "permissions": [
        "activeTab",
        "contextMenus",
        "scripting"
    ],
    "host_permissions":[
        "https://*.pixiv.net/*",
        "https://*.fantia.jp/*",
        "https://*.fanbox.cc/*",
        "https://*.patreon.com/*",
        "https://*.kemono.su/*",
        "https://*.dlsite.com/*",
        "https://*.gelbooru.com/*",
        "https://*.donmai.us/*",
        "https://*.misskey.io/*",
        "https://*.x.com/*",
        "https://*.twitter.com/*",
        "https://*.facebook.com/*"
    ],
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}